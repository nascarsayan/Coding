public class Employee extends Person
{
  private int ecNo;
  private String doj;
  public Employee(String name, int age, int ecno, String doj)
  {
    super(name, age);
    this.ecNo = ecno;
    this.doj = doj;
  }
  pubic void printAttr()
  {
    super.printAttr();
    System.out.println("Employee ID = %d\Date of Joining = %s\n", ecno, doj);
  }
}
